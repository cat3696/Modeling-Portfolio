extends layout.pug

block content 

    div(class="jumbotron") 
        p Put modeling port slideshow here and a list of accomplishments/publications/work history.
    div(id="myCarousel" class="carousel slide" data-ride="carousel")
        ol(class="carousel-indicators")
            li(data-target="#myCarousel" data-slide-to="0" class="active")
            li(data-target="#myCarousel" data-slide-to="1")
            li(data-target="#myCarousel" data-slide-to="2")
            li(data-target="#myCarousel" data-slide-to="3")
            li(data-target="#myCarousel" data-slide-to="4")
            li(data-target="#myCarousel" data-slide-to="5")
            li(data-target="#myCarousel" data-slide-to="6")

        div(class="carousel-inner")
            div(class="carousel-item active")
                img(class="d-block w-100" src="/images/GenevaCrowe1.jpg" alt="Girl with flowers")
                div(class="carousel-caption")
                    h3 Photographer
                    p Geneva Crowe

       
            div(class="carousel-item")
                img(class="d-block w-100" src="/images/coverexclusive.jpg" alt="Girl by window")
                div(class="carousel-caption")
                    h3 Photographer
                    p Hal Masover

        
            div(class="carousel-item")
                img(class="d-block w-100" src="/images/briandemint1.jpg" alt="Girl in paint splatter")
                div(class="carousel-caption")
                    h3 Photographer
                    p Brian DeMint

        
            div(class="carousel-item")
                img(class="d-block w-100" src="/images/MarieKay.jpg" alt="Girl in red dress")
                div(class="carousel-caption")
                    h3 Photographer
                    p Kay Marie

        
            div(class="carousel-item")
                img(class="d-block w-100" src="/images/michelleselesnick.jpg" alt="Girl in white vest posing")
                div(class="carousel-caption")
                    h3 Photographer
                    p Michelle Selsenick

        
            div(class="carousel-item")
                img(class="d-block w-100" src="/images/michelleselsenick2.jpg" alt="Girl holding flowers")
                div(class="carousel-caption")
                    h3 Photographer
                    p Michelle Selsenick

            div(class="carousel-item")
                img(class="d-block w-100" src="/images/WilliamRafaelHerrera.jpg" alt="Wedding couple under St. Louis Arch")
                div(class="carousel-caption")
                    h3 Photographer
                    p William Rafael Herrera
   
        a(class="carousel-control-prev" href="#myCarousel" data-slide="prev")
            span(class="carousel-control-prev-icon" aria-hidden="true")
            span(class="sr-only") Previous
        
        a(class="carousel-control-next" href="#myCarousel" data-slide="next")
            span(class="carousel-control-next-icon" aria-hidden="true")
            span(class="sr-only") Next 
    
